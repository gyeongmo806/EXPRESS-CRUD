        title CRUD-APP
    body
        .container-fluid
            .navbar.navbar-light.bg-light.navbar-expand-lg
                span.navbar-brand.mb-0.h1 POTFOLIO
                ul.navbar-nav
                    li.nav-item.active
                        a.nav-link(href="/") Home
                     
        .container 
            .section
                .title.text-center
                    h2 게시글 목록
                .article
                    .contents-list
                        ul.list-group
                            each item in data
                                a(href=`/read/${item.id}`).list-group-item
                                    span= item.title
                .text-right
                    a.btn.btn-primary(href="/create") write
        script(src='/query.js')
                                
                                    
                                